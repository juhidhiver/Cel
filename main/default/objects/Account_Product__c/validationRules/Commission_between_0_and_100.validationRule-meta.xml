<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Commission_between_0_and_100</fullName>
    <active>true</active>
    <errorConditionFormula>OR(Commission_percentage__c  &lt; 0,Commission_percentage__c &gt; 1)</errorConditionFormula>
    <errorMessage>Commission % must be between 0% and 100%</errorMessage>
</ValidationRule>
